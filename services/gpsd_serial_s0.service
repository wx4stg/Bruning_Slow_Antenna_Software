[Unit]
Description=Run gpsd on serial port 0
After=syslog.target

[Service]
ExecStart=gpsd -Nnb -D 5 /dev/ttyS0
Restart=always
User=root
SyslogIdentifier=gpsd_serial_s0
StandardError=append:/home/pi/Desktop/SA_gps_log.out
StandardOutput=append:/home/pi/Desktop/SA_gps_log.out


[Install]
WantedBy=multi-user.target
